}

@keyframes mmfadeOut { 
  0% { opacity: 1; }
  100% { opacity: 0; }
}

@keyframes mmslideIn { 
  0% { transform: translateY(15%); }
  100% { transform: translateY(0px); }
}

@keyframes mmslideOut { 
  0% { transform: translateY(0px); }
  100% { transform: translateY(-10%); }
}

body.no-scroll { overflow: hidden; }

#header.ino-header-platform { height: var(--header-height); min-height: 60px; }

.with-information-banner #header.ino-header-platform { min-height: 90px; }

